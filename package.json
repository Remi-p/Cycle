    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "build:dev": "webpack --config webpack.dev.js",
        "lint:prettier": "yarn prettier -l 'src/**/*.{ts,js,md,json}'",
        "lint:prettier:fix": "yarn prettier --write 'src/**/*.{ts,js,md,json}'",
        "ci:deploy": "yarn build && gh-pages -d dist -m \"Deploy cycle-game/Cycle\" -u \"Deployment Bot (from GH Actions) <john@doe.org>\" --repo https://$GITHUB_TOKEN@github.com/cycle-game/Cycle.git",
        "deploy": "yarn build && gh-pages -d dist",
        "release": "semantic-release",
        "start:dev": "webpack-dev-server --open --config webpack.dev.js",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook --quiet",
        "test": "jest --forceExit --config jest.json",
        "test:cov": "jest --forceExit --config jest.json --ci --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js}": [
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "avoid"
    },
    "devDependencies": {
