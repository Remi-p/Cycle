        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "expose-loader": "0.7.5",
        "file-loader": "6.0.0",
        "gh-pages": "2.2.0",
        "html-webpack-plugin": "4.3.0",
        "husky": "4.2.5",
        "identity-obj-proxy": "3.0.0",
        "jest": "26.0.1",
        "jest-environment-enzyme": "7.1.2",
        "jest-enzyme": "7.1.2",
        "lint-staged": "10.2.2",
        "node-sass": "4.14.1",
        "prettier": "2.0.5",
        "react-docgen-typescript-loader": "3.7.2",
        "react-is": "16.13.1",
        "redux-devtools-extension": "2.13.8",
        "sass": "1.26.5",
        "sass-loader": "8.0.2",
        "semantic-release": "17.0.7",
        "style-loader": "1.2.1",
        "ts-jest": "26.0.0",
        "ts-loader": "7.0.4",
        "typescript": "3.9.2",
        "webpack": "4.43.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.11.0",
        "webpack-merge": "4.2.2"
    },
