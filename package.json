        "@storybook/addon-toolbars": "6.0.0-beta.7",
        "@storybook/react": "6.0.0-beta.2",
        "@types/enzyme": "3.10.5",
        "@types/jest": "25.2.2",
        "@types/lodash": "4.14.151",
        "@types/react": "16.9.35",
