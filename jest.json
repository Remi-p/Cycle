{
    "preset": "ts-jest",
    "testEnvironment": "enzyme",
    "moduleFileExtensions": ["js", "ts", "tsx"],
    "modulePaths": ["."],
    "testMatch": ["<rootDir>/src/**/*.test.ts?(x)"],
    "moduleNameMapper": {
        "\\.scss$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": ["jest-enzyme"]
}
